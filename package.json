{
  "name": "duckHunters",
  "version": "1.0.0",
  "description": "You need to hunt other ducks",
  "author": "Duck Hunters <theduckhunter101@gmail.com>",
  "scripts": {
    "tslint": "tslint",
    "typedoc": "typedoc",
    "typings": "typings",
    "webpack": "webpack",
    "rimraf": "rimraf",
    "clean:dist": "npm run rimraf -- dist",
    "webpack-dev-server": "webpack-dev-server",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:prod": "npm run build:prod -- --watch",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "webpack --config config/webpack.prod.js  --progress --profile --colors --display-error-details --display-cached --bail",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --inline --progress --profile --colors --watch --display-error-details --display-cached --content-base src/",
    "start": "npm run server:dev",
    "postinstall": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rumblex/angularattack2016-theduckhunters"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "angular2-in-memory-web-api": "0.0.7",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.0",
    "dev": "^0.1.3",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "socket.io-client": "^1.4.6",
    "ts-loader": "^0.8.1",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "angular2-hmr": "~0.7.0",
    "rimraf": "^2.5.2",
    "awesome-typescript-loader": "~0.17.0",
    "compression-webpack-plugin": "^0.3.1",
    "es6-promise": "^3.1.2",
    "es6-promise-loader": "^1.0.1",
    "es6-shim": "^0.35.0",
    "es7-reflect-metadata": "^1.6.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.5",
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
    "typescript": "^1.8.10",
    "typings": "^0.8.1",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "imports-loader": "^0.6.5",
    "ts-helpers": "1.1.1",
    "ts-node": "^0.7.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "parse5": "^1.5.1",
    "raw-loader": "0.5.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.12.0",
    "html-webpack-plugin": "^2.17.0"
  },
  "engines": {
    "node": "^4.2.1",
    "npm": "^3"
  }
}
